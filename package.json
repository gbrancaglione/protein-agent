{
  "name": "protein-agent",
  "version": "1.0.0",
  "description": "An AI agent to help track daily protein intake",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "server": "tsx src/server.ts",
    "server:dev": "tsx watch src/server.ts",
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "seed:dev": "tsx src/scripts/seedDev.ts",
    "worker": "tsx src/workers/index.ts"
  },
  "keywords": [
    "protein",
    "fitness",
    "langchain",
    "ai",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "@langchain/core": "^1.1.0",
    "@langchain/openai": "^1.1.3",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@types/express": "^5.0.6",
    "bullmq": "^5.0.0",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "ioredis": "^5.3.2",
    "langchain": "^1.1.1",
    "pg": "^8.16.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^20.19.25",
    "@types/pg": "^8.15.6",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "eslint": "^9.18.0",
    "prisma": "^7.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.20.0",
    "vitest": "^4.0.14"
  }
}
