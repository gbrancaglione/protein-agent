# OpenAI API Configuration
# Required: Your OpenAI API key for the LLM agent
OPENAI_API_KEY=

# Database Configuration
# Required: PostgreSQL connection string
# DATABASE_URL="postgresql://user:password@localhost:5432/protein_agent?schema=public"

# User Configuration
# Optional: User ID to use (defaults to 1)
USER_ID=1

# Optional: Daily protein target in grams (defaults to 160g if not set in user profile)
DAILY_PROTEIN_TARGET_G=160

# Optional: User weight in kilograms (used if not set in user profile)
USER_WEIGHT_KG=80

# Environment
# Optional: Set to "production" to prevent seed scripts from running
NODE_ENV=development
AUTHENTICATION_API_KEY=123123123
EVOLUTION_API_INSTANCE=

# Debugging
# Optional: Set to any value to enable detailed error stack traces
DEBUG=true

